<view style='margin-bottom:20rpx;width:100%;'>
  <view wx:for="{{anli}}" wx:key="item" style='width:44%;margin:20rpx;border:1rpx solid whitesmoke;background:white;float:left;border-radius: 20rpx;' bindtap='toanli' data-xiang='{{item}}' data-index='{{index}}'>
    <view style='width:100%;height:300rpx;'>
      <image src="{{item.url}}" style='width:100%;height:100%;border-top-left-radius: 20rpx;border-top-right-radius: 20rpx;'></image>
    </view>
    <view style='padding:20rpx;font-size:30rpx;'>
      {{item.name}}
    </view>
  </view>
</view>


